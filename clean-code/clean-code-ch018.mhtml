ogramlisting">public class Args {
  private String schema;
  private Map&lt;Character, ArgumentMarshaler&gt; marshalers =3D
    new HashMap&lt;Character, ArgumentMarshaler&gt;();
  private Set&lt;Character&gt; argsFound =3D new HashSet&lt;Character&gt;()=
;
  private Iterator&lt;String&gt; currentArgument;
  private List&lt;String&gt; argsList;

  public Args(String schema, String[] args) throws ArgsException {
    this.schema =3D schema;
    argsList =3D Arrays.asList(args);
    parse();
  }

  private void parse() throws ArgsException {
    parseSchema();
    parseArguments();
  }

  private boolean parseSchema() throws ArgsException {
    for (String element : schema.split(",")) {
      if (element.length() &gt; 0) {
        parseSchemaElement(element.trim());
      }
    }
    return true;
  }

  private void parseSchemaElement(String element) throws ArgsException {
    char elementId =3D element.charAt(0);
    String elementTail =3D element.substring(1);
    validateSchemaElementId(elementId);
    if (elementTail.length() =3D=3D 0)
      marshalers.put(elementId, new BooleanArgumentMarshaler());
    else if (elementTail.equals("*"))
      marshalers.put(elementId, new StringArgumentMarshaler());
    else if (elementTail.equals("#"))
      marshalers.put(elementId, new IntegerArgumentMarshaler());
    else if (elementTail.equals("##"))
      marshalers.put(elementId, new DoubleArgumentMarshaler());
    else
      throw new ArgsException(ArgsException.ErrorCode.INVALID_FORMAT,
                              elementId, elementTail);
  }

  private void validateSchemaElementId(char elementId) throws ArgsException=
 {
    if (!Character.isLetter(elementId)) {
      throw new ArgsException(ArgsException.ErrorCode.INVALID_ARGUMENT_NAME=
,
                              elementId, null);
    }
  }

  private void parseArguments() throws ArgsException {
    for (currentArgument =3D argsList.iterator(); currentArgument.hasNext()=
;) {
      String arg =3D currentArgument.next();
      parseArgument(arg);
    }
  }

  private void parseArgument(String arg) throws ArgsException {
    if (arg.startsWith("-"))
      parseElements(arg);
  }

  private void parseElements(String arg) throws ArgsException {
    for (int i =3D 1; i &lt; arg.length(); i++)
      parseElement(arg.charAt(i));
  }

  private void parseElement(char argChar) throws ArgsException {
    if (setArgument(argChar))
      argsFound.add(argChar);
    else {
      throw new ArgsException(ArgsException.ErrorCode.UNEXPECTED_ARGUMENT,
                              argChar, null);
    }
  }

  private boolean setArgument(char argChar) throws ArgsException {
    ArgumentMarshaler m =3D marshalers.get(argChar);
    if (m =3D=3D null)
      return false;
    try {
      m.set(currentArgument);
      return true;
    } catch (ArgsException e) {
      e.setErrorArgumentId(argChar);
      throw e;
    }
  }
  public int cardinality() {
    return argsFound.size();
  }

  public String usage() {
    if (schema.length() &gt; 0)
      return "-[" + schema + "]";
    else
      return "";
  }

  public boolean getBoolean(char arg) {
    ArgumentMarshaler am =3D marshalers.get(arg);
    boolean b =3D false;
    try {
      b =3D am !=3D null &amp;&amp; (Boolean) am.get();
    } catch (ClassCastException e) {
      b =3D false;
    }
    return b;
  }

  public String getString(char arg) {
    ArgumentMarshaler am =3D marshalers.get(arg);
    try {
      return am =3D=3D null ? "" : (String) am.get();
    } catch (ClassCastException e) {
      return "";
    }
  }

  public int getInt(char arg) {
    ArgumentMarshaler am =3D marshalers.get(arg);
    try {
      return am =3D=3D null ? 0 : (Integer) am.get();
    } catch (Exception e) {
      return 0;
    }
  }


  public double getDouble(char arg) {
