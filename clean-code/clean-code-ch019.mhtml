=3D"programlisting">package junit.framework;

public class ComparisonCompactor {

  private static final String ELLIPSIS =3D "...";
  private static final String DELTA_END =3D "]";
  private static final String DELTA_START =3D "[";

  private int contextLength;
  private String expected;
  private String actual;
  private int prefixLength;
  private int suffixLength;

  public ComparisonCompactor(
    int contextLength, String expected, String actual
  ) {
    this.contextLength =3D contextLength;
    this.expected =3D expected;
    this.actual =3D actual;
  }

  public String formatCompactedComparison(String message) {
    String compactExpected =3D expected;
    String compactActual =3D actual;
    if (shouldBeCompacted()) {
      findCommonPrefixAndSuffix();
      compactExpected =3D compact(expected);
      compactActual =3D compact(actual);
    }
    return Assert.format(message, compactExpected, compactActual);
  }

  private boolean shouldBeCompacted() {
    return !shouldNotBeCompacted();
  }

  private boolean shouldNotBeCompacted() {
    return expected =3D=3D null ||
            actual =3D=3D null ||
            expected.equals(actual);
  }

  private void findCommonPrefixAndSuffix() {
    findCommonPrefix();
    suffixLength =3D 0;
    for (; !suffixOverlapsPrefix(); suffixLength++) {
      if (charFromEnd(expected, suffixLength) !=3D
          charFromEnd(actual, suffixLength)
      )
        break;
    }
  }

  private char charFromEnd(String s, int i) {
    return s.charAt(s.length() - i - 1);
  }

  private boolean suffixOverlapsPrefix() {
    return actual.length() - suffixLength &lt;=3D prefixLength ||
      expected.length() - suffixLength &lt;=3D prefixLength;
  }

  private void findCommonPrefix() {
    prefixLength =3D 0;
    int end =3D Math.min(expected.length(), actual.length());
    for (; prefixLength &lt; end; prefixLength++)
      if (expected.charAt(prefixLength) !=3D actual.charAt(prefixLength))
        break;
  }

  private String compact(String s) {
    return new StringBuilder()
      .append(startingEllipsis())
      .append(startingContext())
      .append(DELTA_START)
      .append(delta(s))
      .append(DELTA_END)
      .append(endingContext())
      .append(endingEllipsis())
      .toString();
  }

  private String startingEllipsis() {
    return prefixLength &gt; contextLength ? ELLIPSIS : "";
  }

  private String startingContext() {
    int contextStart =3D Math.max(0, prefixLength - contextLength);
    int contextEnd =3D prefixLength;
    return expected.substring(contextStart, contextEnd);
  }

  private String delta(String s) {
    int deltaStart =3D prefixLength;
    int deltaEnd =3D s.length() - suffixLength;
    return s.substring(deltaStart, deltaEnd);
  }

